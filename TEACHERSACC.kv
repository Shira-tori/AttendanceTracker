#: import FallOutTransition kivy.uix.screenmanager.FallOutTransition
#import open_file_manager teacheracc.ProfilePage
#: import file_chooser teacheracc.TEACHERSACC



<Content>:
	home_list: home_list
	settings_list: settings_list
	profile_list: profile_list
	ScrollView:
		MDList:
			OneLineIconListItem:
				id: home_list
				text: "Home"
				theme_text_color: "Custom"
				text_color: "white"
				#bg_color: 1, 1, 1, 1
				divider: "Full"
				divider_color: "black"
				on_press:
					root.nav_drawer.set_state("close")
					root.manager.current = "Home"
					
				IconLeftWidgetWithoutTouch:
					icon: "home"

			OneLineIconListItem:
				id: profile_list
				text: "Profile"
				theme_text_color: "Custom"
				text_color: "white"
				#bg_color: 1, 1, 1, 1
				divider: "Full"
				divider_color: "black"
				on_press:
					root.nav_drawer.set_state("close")
					root.manager.current = "Profile"
					
				IconLeftWidgetWithoutTouch:
        			icon: "account-circle"
						

			OneLineIconListItem:
				id: settings_list
				text: "Settings"
				theme_text_color: "Custom"
				text_color: "white"
				#bg_color: 1, 1, 1, 1
				divider: "Full"
				divider_color: "black"
				#size_hint_x: 1
				on_press:
					root.nav_drawer.set_state("close")
					root.manager.current = "Settings"
					
				IconLeftWidgetWithoutTouch:
					icon: "cog"


			OneLineIconListItem:
				id: logout_list
				text: "Logout"
				theme_text_color: "Custom"
				text_color: "red"
				#bg_color: 1, 1, 1, 1
				divider: "Full"
				divider_color: "black"
				#size_hint_x: 1
				on_press:
					on_release: app.stop()
					
				IconLeftWidgetWithoutTouch:
					icon: "logout"



<MainScreen>:
	profpage: profpage
	nav_layout: nav_layout
	NavLayout:
		id: nav_layout
		screen_man: screen_man
		content: content
		ScreenMan:
			id: screen_man
			transition: FallOutTransition()
			Screen:
				name: "Home"
				id: teachers_screen	
				MDBoxLayout:
					orientation: "vertical"
					canvas.before:
						Color:
							rgba: .1326, .3907, .4767, 1
						Rectangle:
							pos: self.pos
							size: self.size

					MDBoxLayout:
						adaptive_size: True

					MDTopAppBar:
						#opposite _colors: True
						set_bars_color: True
						use_overflow: True
						#type_height: "large"		        		        
						title: "Teacher's Account"
						adaptive_height: True
						adaptive_width: True
						md_bg_color: "black"
						#specific_text_color: 1, 1, 1, 1
						#92/255, 92/255, 92/255, 1
						#right_action_items: [["dots-vertical", lambda x: app.callback()]]
						left_action_items: [["menu", lambda x: nav_drawer.set_state("open")]]
						elevation: 25
						
					#MDSeparator:
		#				height: "20dp"

					MDTabs:
						id: tab_page
						specific_text_color: 1, 1, 1, 1
						background_color: "black"
						#92/255, 92/255, 92/255, 1
						tab_indicator_type: "line"
						adaptive_width: True
						adaptive_height: True
						indicator_color: 1, 1, 1, 1
						ripple_duration: 2
						tab_hint_x: True
						underline_color: 92/255, 92/255, 92/255, 1
						elevation: 20

						Tab:
							allow_stretch: True				    
							title: "Home"
							icon: "home"
							FloatLayout:		        
								BaseShadowMDCard:					
									ripple_behavior: True
									orientation: "vertical"
									size_hint: None, None	
									size: "280dp", "200dp"
									pos_hint: {"center_x": .5, "center_y": .7}
									elevation: 30
									on_release: root.current = "gradesection_screen"
									AsyncImage:
										id: list
										source: "list.png"
										size_hint: (.8, 1)
										keep_ratio: False
										allow_stretch: True
										#size: "140dp", "90dp"
										pos_hint: {"center_x": .5, "center_y": .5}
									MDLabel:
										size_hint: (1, 0.2)					
										text: "Grade & Sections List"
										font_name: 'Qazz'
										theme_text_color: "Custom"
										text_color: "teal"
										font_size: "20dp"
										bold: True
										halign: "center"
										valign: "bottom"
										
								BaseShadowMDCard:
									ripple_behavior: True
									orientation: "vertical"							
									size_hint: None, None
									size: "280dp", "200dp"
									pos_hint: {"center_x": .5, "center_y": .3}
									elevation: 30
									AsyncImage:
										id: scanner
										source: "scanner.png"
										size_hint: (.8, 1)
										keep_ratio: False
										allow_stretch: True
										#size: "140dp", "90dp"
										pos_hint: {"center_x": .5, "center_y": .5}
										
									MDLabel:
										size_hint: (1, .2)
										text: "QR Code Scanner"
										font_name: "Qazz"
										theme_text_color: "Custom"                       
										text_color: "teal"
										font_size: "20dp"
										bold: True
										halign: "center"
										valign: "bottom"						       
										
							
				
						Tab:		          
							title: "Notifications"
							icon:"bell"
							Label:
								text: "No Notifications"
							#on_press: root.switch_to(root.tab_list[1])
			Screen:
				id: profpage
				name: "Profile"
				BoxLayout:
					size_hint_y: 1
					size_hint_x: 1
					orientation: "vertical"
					canvas.before:
						Color:
							rgba: .1326, .3907, .4767, 1
						Rectangle:
							pos: self.pos
							size: self.size

					MDTopAppBar:
						#opposite _colors: True
						set_bars_color: True
						use_overflow: True
						#type_height: "large"		        		        
						title: "My Profile"
						#title_font_size: 40
						adaptive_height: True
						adaptive_width: True
						md_bg_color: "black"
						#specific_text_color: 1, 1, 1, 1
						#92/255, 92/255, 92/255, 1
						#right_action_items: [["dots-vertical", lambda x: app.callback()]]
						left_action_items: [["menu", lambda x: nav_drawer.set_state("open")]]
						elevation: 20

					MDLabel:



			        FloatLayout:
			        	BaseShadowMDCard:
			        		id: changepic
			        		ripple_behavior: True
							orientation: "vertical"							
							size_hint: None, None
							size: "280dp", "200dp"
							pos_hint: {"center_x": .5, "center_y": .5}
							elevation: 30
							on_press: app.file_chooser()
							
								
							MDLabel:
								size_hint: (1, .2)
								text: "Choose Own Image"
								font_name: "Qazz"
								theme_text_color: "Custom"                       
								text_color: "teal"
								font_size: "20dp"
								bold: True
								halign: "center"
								valign: "bottom"


					AsyncImage:
						id: userimage
						pos_hint: {"center_x": .5, "center_y": .2}
						source: app.image_source
						ref: userimage

			Screen:
				name: "Settings"
				BoxLayout:
					size_hint_y: 1
					size_hint_x: 1
					orientation: "vertical"
					canvas.before:
						Color:
							rgba: .1326, .3907, .4767, 1
						Rectangle:
							pos: self.pos
							size: self.size

					MDTopAppBar:
						#opposite _colors: True
						set_bars_color: True
						use_overflow: True
						#type_height: "large"		        		        
						title: "Settings"
						adaptive_height: True
						adaptive_width: True
						md_bg_color: "black"
						#specific_text_color: 1, 1, 1, 1
						#92/255, 92/255, 92/255, 1
						#right_action_items: [["dots-vertical", lambda x: app.callback()]]
						left_action_items: [["menu", lambda x: nav_drawer.set_state("open")]]
						elevation: 20
					
					MDLabel:




					# FloatLayout:
					# 	BaseShadowMDCard:
					# 		ripple_behavior: True
					# 		orientation: "vertical"
					# 		size_hint: None, None	
					# 		size: "280dp", "180dp"
					# 		pos_hint: {"center_x": .5, "center_y": .7}
					# 		elevation: 30
					# 		#on_release: root.current = "gradesection_screen"
					# 		MDLabel:
					# 			text: "Account Management -x c"
					# 			font_name: 'Qazz'
					# 			theme_text_color: "Custom"
					# 			text_color: "teal"
					# 			font_size: "30dp"
					# 			bold: True
					# 			halign: "center"
					# 			valign: "middle"

		MDNavigationDrawer:
			id: nav_drawer
			opening_transition: "out_sine"
			opening_time: .6
			closing_transition: "out_sine"
			closing_time: .6
			elevation: 15
			md_bg_color: .1326, .3907, .4767, 1
			MDBoxLayout:
				orientation: "vertical"
				spacing: "8dp"
				padding: "8dp"
				Content:
					id: content					
					manager: screen_man
					nav_drawer: nav_drawer











